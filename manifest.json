{
    "manifest_version": 3,
    "name": "My extension",
    "version": "1.0.0",
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_popup" : "index.html"
    }, 
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*",
        "https://chat.openai.com/*"
    ]
}
